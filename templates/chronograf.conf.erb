HOST=<%= @host %>
PORT=<%= @port %>
REPORTING_DISABLED=<%= @reporting_disabled %>
LOG_LEVEL=<%= @log_level %>
STATUS_FEED_URL=<%= @status_feed_url %>
CANNED_PATH=<%= @canned_path %>
RESOURCES_PATH=<%= @resources_path %>
<% if @bolt_path %>BOLT_PATH=<%= @bolt_path %><% end %>
<% if @tls_certificate_path %>TLS_CERTIFICATE=<%= @tls_certificate_path %><% end %>
<% if @tls_private_key_path %>TLS_PRIVATE_KEY=<%= @tls_private_key_path %><% end %>
<% if @public_url %>PUBLIC_URL=<%= @public_url %><% end %>
<% if @token_secret %>TOKEN_SECRET=<%= @token_secret %><% end %>
<% if @auth_duration %>AUTH_DURATION=<%= @auth_duration %><% end %>
<% if @google_client_id and @google_client_secret %>
GOOGLE_CLIENT_ID=<%= @google_client_id %>
GOOGLE_CLIENT_SECRET=<%= @google_client_secret %>
<% if @google_domains %>GOOGLE_DOMAINS=<%= @google_domains %><% end %>
<% end %>
<% if @github_client_id and @github_client_secret %>
GH_CLIENT_ID=<%= @github_client_id %>
GH_CLIENT_SECRET=<%= @github_client_secret %>
<% if @github_organization %>GH_ORGS=<%= @github_organization %><% end %>
<% end %>
<% if @heroku_client_id and @heroku_client_secret %>
HEROKU_CLIENT_ID=<%= @heroku_client_id %>
HEROKU_CLIENT_SECRET=<%= @heroku_client_secret %>
<% if @heroku_organizations %>HEROKU_ORGS=<%= @heroku_organizations %><% end %>
<% end %>
